                $($('.two_column .panel-body')[k+1]).height(height);
            }
        });
    }
}
$(window).on('load', function() {
    cat2ColTimer = setTimeout(function(){
       fixColumnHeight();
    }, 100)
});
$(function(){
    $(window).resize(function(){
        clearTimeout(cat2ColTimer)
        cat2ColTimer = setTimeout(function(){
           fixColumnHeight();
        }, 100)
    });
});
</script>